{
    "latex-workshop.latex.tools": [
        {
            "name": "texify",
            "command": "texify",
            "args": [
                "--synctex=1",
                "--pdf",
                "--clean",
                "%DOC%.tex"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "texify",
            "tools": [
                "texify"
            ]
        }
    ]
}