{
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "%DOC%"
      ]
    },
    {
      "name": "texify",
      "command": "texify",
      "args": [
        "--synctex=1",
        "--pdf",
        "--clean",
        "%DOC%.tex"
      ]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "texify",
      "tools": [
        "texify"
      ]
    },
    {
      "name": "latexmk",
      "tools": [
        "latexmk"
      ]
    }
  ],
  "latex-workshop.synctex.afterBuild.enabled": true,
  "latex-workshop.message.badbox.show": false,
  "latex-workshop.latex.clean.enabled": true
}
